{
    "name": "create-rds-app",
    "version": "0.5.2",
    "bin": "./bin/cli.js",
    "scripts": {
        "dev": "next dev -p 80",
        "build": "yarn format && yarn lint && next build",
        "start": "next start -p 80",
        "lint": "next lint",
        "format": "prettier --write .",
        "nvmuse": "unset PREFIX && source $HOME/.nvm/nvm.sh && nvm install",
        "predev": "test -n \"$NOPRESCRIPTS\" || yarn run nvmuse",
        "prestart": "test -n \"$NOPRESCRIPTS\" || yarn run nvmuse",
        "preinstall": "test -n \"$NOPRESCRIPTS\" || yarn run nvmuse",
        "prebuild": "test -n \"$NOPRESCRIPTS\" || yarn run nvmuse",
        "pretest": "test -n \"$NOPRESCRIPTS\" || yarn run nvmuse"
    },
    "dependencies": {
        "jwt-decode": "^3.1.2",
        "next": "^12.1.4",
        "next-auth": "^4.3.1",
        "nodemailer": "^6.6.3",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@marceloglacial/rds-beta": "^0.13.0",
        "@types/node": "17.0.13",
        "@types/react": "17.0.38",
        "eslint": "8.7.0",
        "eslint-config-next": "12.0.9",
        "prettier": "^2.5.1",
        "typescript": "4.5.5"
    }
}
